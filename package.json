    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
